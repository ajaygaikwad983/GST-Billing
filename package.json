{
  "name": "om-sai-lamination",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "electron-run": "electron .",
    "dev": "ng serve && electron .",
    "preomsai": "ng s -o",
    "go": " start cmd @cmd /k json-server --watch data.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:win": "electron-builder build --publish=never",
    "build:win64": "electron-builder build --win --x64",
    "build:win32": "electron-builder build --win --ia32"
  },
  "private": true,
  "build": {
    "appId": "com.omsailamination.gst-billing",
    "productName": "Om Sai Lamination",
    "copyright": "Copyright Â© 2020 Om Sai Lamination",
    "win": {
      "requestedExecutionLevel": "requireAdministrator",
      "icon": "assets/logo.png"
    },
    "files": [
      "dist/**/*",
      "data.json",
      "app.js"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "extends": null
  },
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "bootstrap": "^4.5.2",
    "dom-to-image": "^2.6.0",
    "electron-log": "^4.4.8",
    "file-saver": "^2.0.2",
    "html2canvas": "^1.0.0-rc.7",
    "jquery": "^3.5.1",
    "jspdf": "^2.1.0",
    "ngx-electron": "^2.2.0",
    "pdfmake": "^0.1.68",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^8.2.0",
    "electron": "^10.4.7",
    "electron-builder": "^24.4.0",
    "electron-packager": "^17.1.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
